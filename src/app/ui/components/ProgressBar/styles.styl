

.progressbar {
    width: 100%;
    height: 8px;
    overflow: hidden;
    background-color: #fafafa;

    .progress-notice {
        z-index: 500;
        position: absolute;
        left: 8px;
        bottom: 0;
    }
    :global(.ant-progress-inner) {
        top: -8px;
    }
    :global(.ant-progress-outer) {
        margin-right: 0!important;
        padding-right: 0!important;
    }
}
