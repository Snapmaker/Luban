.lu-logo-container {
    display: inline-block
    .logo {
        background-image: url('../../../../resources/images/snap-luban-logo-64x64.png');
        width: 64px;
        height: 64px;
        display: inline-block;
        background-size: 100%;
    }
}
.lu-version {
    margin-left: 16px;
    vertical-align: top;
    display: inline-block;
    width: calc(100% - 100px);
    .autoupdate-auto {
        margin-top: 8px;
    }
}
.autoupdate-wrapper{
    max-width: 480px;
    .autoupdate-update {
        float: left;
        width: 100%;
        text-align: center;
        margin-top: 44px;
        .autoupdate-button {
            width: 90%;
        }
    }

}
