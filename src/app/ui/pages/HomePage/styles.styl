.create-new-project {
    width: 95vw;
    height: auto;
    background: #fff;
    margin: 48px auto 0;
    border-radius: 24px;

    .beginPart {
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        padding: 2.22vw 2.22vw 0;

        .headingPart {
            display: flex;
            width: 100%;
            justify-content: center;
            align-item: center;
        }

        .begin-container {
            width: 100%;

            .link-bar {
                height: 166px;
                display: flex;
                flex-direction: wrap;
                justify-content: center;
                align-items: flex-start;

                .link-bar-item {
                    > a {
                        display: flex;
                        flex-direction: column;
                        justify-content: center;
                        align-items: center;
                        width: 16.66vw;
                        max-width: 240px;

                        .imgWrapper {
                            width: 100%;
                            max-height: 148px;
                            height: 10.28vw;
                            min-height: 120px;
                            display: flex;
                            justify-content: center;
                            align-items: center;
                            margin-bottom: 12px;

                            > img {
                                display: block;
                            }

                            .cnc-axis-select, .laser-axis-select {
                                display: none;
                            }

                            .three-axis-select, .four-axis-select {
                                height: 32px;
                                width: 100%;
                                text-align: center;
                                line-height: 32px;
                                // border 1px solid #b9bcbf
                                border-radius: 8px;
                            }

                            .three-axis-select {
                                margin-bottom: 12px;
                            }

                            &:hover {
                                cursor: pointer;
                                box-shadow: 0 0 23px -5px #a5a5a550;
                                border-radius: 24px;

                                .cnc-axis-select, .laser-axis-select {
                                    display: flex;
                                    flex-direction: column;
                                    justify-content: center;
                                    height: 120px;
                                    width: 120px;
                                }

                                .laser-img, .cnc-img {
                                    display: none;
                                }
                            }
                        }
                    }
                }

                .laser, .cnc {
                    > a {
                        .imgWrapper:hover {
                            cursor: default;
                        }
                    }
                }
            }

            .recent-files {
                height: 166px;
                width: 100%;

                .recent-file-list {
                    display: flex;
                    flex-wrap: wrap;
                    justify-content: flex-start;

                    .file-item {
                        width: 20.14vw;
                        border-bottom: 1px solid #D5D6D9;
                        margin-right: 2.88vw;
                        height: 55.3px;
                        line-height: 75.3px;

                        &:hover {
                            cursor: pointer;
                        }

                        .file-name {
                            display: inline-block;
                            max-width: calc(95% - 90px);
                            overflow: hidden;
                            white-space: nowrap;
                            text-overflow: ellipsis;
                        }

                        .suffix-name {
                            display: inline-block;
                            width: 90px;
                            overflow: hidden;
                            white-space: nowrap;
                            text-overflow: ellipsis;
                        }
                    }

                    .file-item:nth-child(4n+4) {
                        margin-right: 0;
                    }
                }
            }
        }
    }
}

.quick-start-container {
    .case-list {
        position: relative;
        padding: 2vw 3.33vw;
        display: flex;
        flex-wrap: wrap;
        justify-content: space-between;
        align-content: flex-start;

        .case-item {
            margin-bottom: 10px;
            width: 32%;
            display: flex;
            align-items;
            padding: 8px;

            .imgWrapper {
                // height 97px
                // width 97px
                height: 6.74vw;
                width: 6.74vw;
                max-height: 97px;
                min-height: 69px;
                max-width: 97px;
                min-width: 69px;
                display: flex;
                // align-items center
                background: #EEEFF0;
                border-radius: 16px;

                > img {
                    width: 100%;
                }
            }

            .caseText {
                width: 10vw;
                white-space: normal;
                padding: 6px 0 0 12px;
            }

            &:last-child {
                margin-right: auto;
                margin-left: 2%;
            }

            &:nth-child(3n) {
                margin-right: unset;
                margin-left: unset;
            }

            &:hover {
                cursor: pointer;
                border-radius: 24px;
                box-shadow: 0 0 23px -5px #a5a5a550;
            }
        }
    }

    .small-list {
        align-content: space-between;
    }
}

.more-info {
    .resource-list {
        padding: 2vw 3.33vw 7vw;
        display: flex;
        flex-wrap: wrap;
        align-items: center;

        .listItem {
            width: 65%;
        }

        .right-part {
            width: 35%;
        }
    }
}

.resources-container {
    width: 62.4vw;

    .case-list {
        height: 24.7vw;
        max-height: 339px;
        min-height: 256px;
        overflow: auto;
        background: #fff;
        border-radius: 24px;
        padding: 16px 24px 8px;
        display: flex;
        flex-wrap: wrap;
        justify-content: flex-start;
        align-content: flex-start;

        .case-item {
            position: relative;
            margin-block: 8px;
            width: 16.666%;
            display: flex;
            align-items;
            padding: 8px;
            justify-content: center;

            &:first-child {
                width: 33.33%;

                .imgWrapper {
                    max-width: 350px;
                }

                .caseText {
                    display: block;
                }
            }

            .imgWrapper {
                position: relative;
                // height 97px
                // width 97px
                // height 100%
                width: 100%;
                max-height: 175px;
                min-height: 69px;
                max-width: 175px;
                min-width: 69px;
                display: flex;
                // align-items center
                background: #EEEFF0;
                border-radius: 16px;
                overflow: hidden;

                > img {
                    width: 100%;
                }
            }

            .case-more {
                position: absolute;
                left: 0;
                top: 0;
                width: 100%;
                height: 100%;
                padding: 8px;
                display: flex;
                align-items: center;
                justify-content: center;
                background: rgba(255, 255, 255, .8);
                border-radius: 24px;

                :global(.ant-anchor-ink) {
                    visibility: hidden;
                }
            }

            .caseText {
                position: absolute;
                display: none;
                bottom: 0;
                width: 100%;
                text-align: left;
                padding-inline: 6px;
                background: #a3a3a380;
                color: #545659;
                padding-bottom: 3px;
                border-bottom-right-radius: 16px;
                border-bottom-left-radius: 16px;

                .case-author {
                    font-weight: 400;
                    font-size: 10px;
                    line-height: 12px;
                    color: #575759;
                    -webkit-line-clamp: 1;
                    -webkit-box-orient: vertical;
                    text-overflow: ellipsis;
                    overflow: hidden;
                    display: -webkit-box;
                }

                .case-title {
                    font-weight: bold;
                    // font-size: 12px;
                    line-height: 26px;
                    -webkit-line-clamp: 1;
                    -webkit-box-orient: vertical;
                    text-overflow: ellipsis;
                    overflow: hidden;
                    display: -webkit-box;
                }
            }

            &:hover {
                cursor: pointer;
                border-radius: 24px;
                box-shadow: 0 0 23px -5px rgb(151, 151, 151);
            }

            &:hover .caseText {
                display: block;
            }
        }
    }
}
