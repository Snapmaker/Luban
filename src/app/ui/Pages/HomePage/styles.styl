.icon
    width: 5.5vw
    height: 5.5vw
    min-width 35px
    min-height 35px
    display: inline-block
    vertical-align: middle
    filter: invert(60%)
.title-label
    font-size: 18px
    line-height 24px
    font-weight: bold
.sub-title
    font-size 1.2vw
    line-height 1.2vw

.create-new-project
    width 95vw
    height auto
    min-width 670px
    min-height 150px
    margin 16px auto 0
    .beginPart
        width 95vw
        height auto
        min-width 670px;
        min-height 150px
        border-radius: 8px
        margin 24px auto 0
        display: flex
        justify-content space-between
        .beginContainer
            width: 65%
            min-width 450px
            display flex
            flex-direction column
            .link-bar
                height 100%
                background #fff
                margin-top 15px
                display: flex
                align-items: center
                justify-content space-between
                .icon-3dp
                    background: url('./images/3dp.svg') no-repeat 0 0
                .icon-laser
                    background: url('./images/laser.svg') no-repeat 0 0
                .icon-cnc
                    background: url('./images/cnc.svg') no-repeat 0 0
                .icon-xyz
                    background: url('./images/xyz.svg') no-repeat 0 0

                .workspace, .3dp, .laser, .cnc
                    width 13vw
                    min-width 90px
                    height 15.6vw
                    min-height 90px
                    padding-bottom 2.2vw
                    > a
                        height 100%
                        display flex
                        flex-direction column
                        align-items center
                        justify-content flex-end
                        .common-label 
                            font-size 18px
                            line-height 22px
                            font-weight bold
                        > i
                            margin-bottom 2.64vw
                    &:hover
                        .common-label
                            color: #3aabff
                        .icon
                            filter: invert(.5) sepia(1) saturate(5) hue-rotate(175deg)
                .laser
                    &-axis-select
                        display none
                    &:hover
                        .icon-laser
                            display none
                        .laser-axis-select
                            display flex
                            flex-direction column
                            width 6vw
                            height 5.5vw
                            margin-bottom 2.64vw
                            .axis
                                height 50%
                                display flex
                                justify-content center
                                align-items center
                                border 2px solid #86868b
                                font-size 16px
                                font-weight bold
                                &:hover
                                    color #3aabff
                                    border 2px solid #3aabff
                .cnc
                    &-axis-select
                        display none
                    &:hover
                        .icon-cnc
                            display none
                        .cnc-axis-select
                            display flex
                            flex-direction column
                            width 6vw
                            height 5.5vw
                            margin-bottom 2.64vw
                            .axis
                                height 50%
                                display flex
                                justify-content center
                                align-items center
                                border 2px solid #86868b
                                font-size 16px
                                font-weight bold
                                &:hover
                                    color #3aabff
                                    border 2px solid #3aabff
        .nearly-container
            // flex-grow 3
            width 32%
            font-size 1.2vw
            line-height 1.2vw
            .file-container
                height 15.6vw
                min-height 90px
                font-size 1vw
                line-height 1.2vw
                margin-top 15px
                padding 2.36vw 1.67vw 1.67vw
                background #fff
                display flex
                flex-direction column
                justify-content space-between
                .open-file-btn
                    width 100%
                    > button
                        width 100%
                        background transparent
                        border-radius 18.5px
                .recent-file-list
                    height 8vw
                    display flex
                    flex-wrap wrap
                    .file-item
                        width 50%
                        overflow hidden
                        white-space nowrap
                        text-overflow ellipsis
                        cursor pointer


.quick-start-container
    width 95vw
    min-width 670px
    margin 50px auto 0
    .small-list
        width 48.75%
    .case-list
        display flex
        align-item center
        justify-content space-between
        margin-top 24px

        .case-item
            width 13.37vw
            height auto
            min-width 90px
            min-height 90px
            cursor pointer
            font-size 12px
            position relative
            .case-title
                font-size 14px
                line-height 22px
                margin: 0.9vw 0 0.5vw
                width 13vw
                overflow hidden
                text-overflow ellipsis
                white-space nowrap

            .tag-icon
                background #D6D6D7
                padding 2px 4px
                border-radius 2px
                margin-top 4px
                font-size 12px
                line-height 0.9vw
                position absolute
                top -3px
                left 0px
                color #86868B
            img
                // height 138px
                // width: 200px
                width 100%
                height 60%
                border 2px solid #D6D6D7
                border-radius 4px
                background #fff
                padding 20px

.more-info
    width 95vw
    min-width 670px
    margin 50px auto 20px
    font-size 1.3vw
    .resource-list
        display flex
        justify-content space-between
        align-items center
        margin-top 24px
        > a
            width 13.37vw
            height auto
            display flex
            flex-direction column
            align-items center
            min-width 90px
            font-size 14px
            line-height 28px
            > img
                width 100%
                padding 2.7vw 4.6vw
                border 2px solid #D6D6D7
                margin-bottom 0.9vw
                height 9.2vw
.setting-modal
    .title-label
        font-weight normal
        margin-bottom 26px
    .lang-select
        display flex
        flex-direction column
        align-items center
        .lang-select-input
            width 160px
            height 30px
    .machine-select
        display flex
        flex-direction column
        align-items center
        .machine-content
            width 100%
            display flex
            justify-content space-evenly
            align-items center
            .machine-img
                display flex
                flex-direction column
                align-items center
                justify-content space-between
                > img
                    width 160px
            .machine-info
                min-width 200px
                .z-axis-select
                    display flex
                    align-items center
                    font-size 14px
                    margin-bottom 14px
                    > input
                        margin-right 8px
                        cursor pointer

            
