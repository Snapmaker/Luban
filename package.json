{
  "name": "snapmaker-luban",
  "version": "4.11.2",
  "description": "A web-based interface for Snapmaker 3-in-1 3D Printer",
  "homepage": "https://github.com/Snapmaker/Luban",
  "author": "Snapmaker Software Team",
  "main": "./dist/Luban/src/server-cli.js",
  "bin": {
    "luban": "bin/cli"
  },
  "workspaces": [
    "packages/luban-print-settings",
    "packages/luban-platform"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Snapmaker/Luban.git"
  },
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": ">=16"
  },
  "os": [
    "darwin",
    "win32",
    "linux"
  ],
  "scripts": {
    "postinstall": "npm run pkgsync && npm run electron-rebuild",
    "commit": "npx git-cz",
    "pkgsync": "node build/pkgsync.js",
    "update-i18n": "gulp i18nextServer i18nextApp",
    "prepare": "npm run clean",
    "prebuild": "bash build/prebuild-prod.sh",
    "prebuild-dev": "bash build/prebuild-dev.sh",
    "prebuild-prod": "bash build/prebuild-prod.sh",
    "build": "gulp production --max_old_space_size=8092",
    "build-dev": "gulp development",
    "build-prod": "gulp production",
    "electron-builder": "electron-builder --publish never",
    "electron-rebuild": "electron-rebuild",
    "build:mac": "bash -c 'build/electron-builder.sh --mac --x64 --arm64'",
    "build:mac-x64": "bash -c 'build/electron-builder.sh --mac --x64'",
    "build:mac-arm64": "bash -c 'build/electron-builder.sh --mac --arm64'",
    "build:linux-ia32": "bash -c 'build/electron-builder.sh --linux --ia32'",
    "build:linux-x64": "bash -c 'build/electron-builder.sh --linux --x64'",
    "build:win-x64": "bash -c 'build/electron-builder.sh --win --x64'",
    "clean": "bash -c 'rm -rf ./dist ./output'",
    "start-electron": "electron ./dist/Luban/src/main",
    "start-dev": "concurrently --kill-others --raw \"npm run serve\" \"npm run watch:server\" \"npm run watch:server-worker\" \"gulp serverStartDevelopment\"",
    "start-prod": "cross-env NODE_ENV=production ./bin/cli",
    "dev": "npm run build-dev && npm run start-dev",
    "prod": "npm run build-prod && npm run start-prod",
    "lint": "npm run eslint && npm run stylint",
    "eslint": "eslint --ext .js --ext .ts --ext .jsx --ext .tsx src packages gulp *.js",
    "eslint:debug": "echo \"Checking code style...\"; DEBUG=eslint:cli-engine eslint --ext .js --ext .ts --ext .jsx --ext .tsx packages src gulp *.js",
    "stylint": "stylint src/app",
    "test": "tape -r @babel/register test/*.js --no-timeout --node-arg=--require --node-arg=babel-register --node-arg=--require --node-arg=babel-polyfill",
    "coveralls": "tape -r @@babel/register test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls",
    "serve": "npx webpack serve --config webpack.config.app.development.js --progress",
    "watch:server-worker": "webpack --config webpack.config.server.worker.development.js",
    "watch:server": "tsc -p ./tsconfig-server.json --watch",
    "build:server": "tsc -p ./tsconfig-server.json",
    "build:main": "cross-env NODE_ENV=production babel \"./src/*.js\" --config-file ./babel.config.js -d ./output/",
    "build:app": "webpack --config webpack.config.app.production.js"
  },
  "keywords": [
    "Snapmaker",
    "Luban",
    "3D Printing",
    "Laser",
    "CNC",
    "G-code",
    "Marlin"
  ],
