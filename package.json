{
  "name": "snapmaker-luban",
  "version": "3.15.0",
  "description": "A web-based interface for Snapmaker 3-in-1 3D Printer",
  "homepage": "https://github.com/Snapmaker/Luban",
  "author": "Snapmaker Dev Team <whimsycwd@snapmaker.com>",
  "main": "./dist/Luban/server-cli.js",
  "bin": {
    "luban": "bin/cli"
  },
  "files": [
    "bin",
    "dist",
    "static"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Snapmaker/Luban.git"
  },
  "license": "AGPL-3.0-or-later",
  "engines": {
    "node": ">=4"
  },
  "preferGlobal": true,
  "scripts": {
    "commit": "npx git-cz",
    "pkgsync": "node build/pkgsync.js",
    "update-i18n": "gulp i18nextServer i18nextApp",
    "prepare": "npm run clean",
    "prebuild": "bash build/prebuild-prod.sh",
    "prebuild-dev": "bash build/prebuild-dev.sh",
    "prebuild-prod": "bash build/prebuild-prod.sh",
    "build": "gulp production",
    "build-dev": "gulp development",
    "build-prod": "gulp production",
    "electron-builder": "electron-builder --publish never",
    "electron-rebuild": "electron-rebuild",
    "build:mac-x64": "bash -c 'build/electron-builder.sh --mac --x64'",
    "build:linux-ia32": "bash -c 'build/electron-builder.sh --linux --ia32'",
    "build:linux-x64": "bash -c 'build/electron-builder.sh --linux --x64'",
    "build:win-ia32": "bash -c 'build/electron-builder.sh --win --ia32'",
    "build:win-x64": "bash -c 'build/electron-builder.sh --win --x64'",
    "clean": "bash -c 'rm -rf ./dist ./output'",
    "start-electron": "electron ./dist/Luban/main",
    "start-dev": "gulp serverStartDevelopment",
    "start-prod": "cross-env NODE_ENV=production ./bin/cli",
    "dev": "npm run build-dev && npm run start-dev",
    "prod": "npm run build-prod && npm run start-prod",
    "lint": "npm run eslint && npm run stylint",
    "eslint": "eslint --ext .js --ext .jsx *.js src gulp test",
    "eslint:debug": "echo \"Checking code style...\"; DEBUG=eslint:cli-engine eslint --ext .js --ext .jsx *.js src gulp test",
    "stylint": "stylint src/app",
    "test": "tape -r @babel/register test/*.js --no-timeout --node-arg=--require --node-arg=babel-register --node-arg=--require --node-arg=babel-polyfill",
    "coveralls": "tape -r @@babel/register test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls"
  },
  "build": {
    "appId": "com.snapmaker.luban",
    "asar": false,
    "asarUnpack": [],
    "productName": "Snapmaker Luban",
    "directories": {
      "buildResources": "build",
      "output": "output",
      "app": "dist/Luban"
    },
    "publish": [
      {
        "provider": "github",
        "url": ""
      }
    ],
    "fileAssociations": [
      {
        "ext": [
          "snap3dp",
          "snapcnc",
          "snaplzr"
        ],
        "name": "Project files",
        "role": "Viewer"
      }
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/electron/snapmaker-luban.icns",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist"
    },
    "afterSign": "build/notarize.js",
    "dmg": {
      "background": "build/electron/background.png",
      "icon": "build/electron/snapmaker-luban.icns",
      "iconSize": 80,
      "iconTextSize": 12,
      "contents": [
        {
          "x": 448,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 344,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "verifyUpdateCodeSignature": false,
      "icon": "build/electron/snapmaker-luban.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    },
    "linux": {
      "category": "Utility",
      "maintainer": "parachvte <ryan@snapmaker.com>",
      "target": [
        "deb",
        "rpm"
      ]
    }
  },
  "keywords": [
    "Snapmaker",
    "Luban",
    "3D Printing",
    "Laser",
    "CNC",
    "G-code",
    "Marlin"
  ],
  "dependencies": {
