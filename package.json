{
  "name": "snapmakerjs",
  "version": "2.5.4-alpha.1",
  "description": "A web-based interface for Snapmaker 3-in-1 3D Printer",
  "homepage": "https://github.com/whimsycwd/Snapmakerjs",
  "author": "Snapmaker Dev Team <whimsycwd@snapmaker.com>",
  "contributors": [
    "David Chen <whimsycwd@snapmaker.com>",
    "parachvte <ryan@snapmaker.com>",
    "walker <walker@snapmaker.com>"
  ],
  "main": "./dist/cnc/cnc.js",
  "bin": {
    "cnc": "./bin/cnc",
    "cncjs": "./bin/cnc"
  },
  "files": [
    "bin",
    "dist",
    "static"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:whimsycwd/Snapmakerjs.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "preferGlobal": true,
  "scripts": {
    "pkgsync": "babel-node scripts/pkgsync.js",
    "gettext": "node scripts/gettext.js",
    "prepare": "npm run clean && npm run lint && npm run test && npm run build",
    "prebuild": "npm run prebuild-prod",
    "prebuild-dev": "bash scripts/prebuild-dev.sh",
    "prebuild-prod": "bash scripts/prebuild-prod.sh",
    "build": "gulp prod",
    "build-dev": "gulp dev",
    "build-prod": "gulp prod",
    "build-i18n": "gulp app:i18n web:i18n",
    "electron-builder": "build",
    "electron-rebuild": "electron-rebuild",
    "build:mac-x64": "bash -c 'scripts/electron-builder.sh --mac --x64'",
    "build:linux-ia32": "bash -c 'scripts/electron-builder.sh --linux --ia32'",
    "build:linux-x64": "bash -c 'scripts/electron-builder.sh --linux --x64'",
    "build:win-ia32": "bash -c 'scripts/electron-builder.sh --win --ia32'",
    "build:win-x64": "bash -c 'scripts/electron-builder.sh --win --x64'",
    "clean": "bash -c 'rm -rf ./dist ./output'",
    "start": "./bin/cnc",
    "start-electron": "electron ./dist/cnc/main",
    "watch-dev": "webpack --watch --config webpack.appconfig.development.js",
    "start-dev": "cross-env NODE_ENV=development ./bin/cnc -vvv -p 8000",
    "dev": "npm run build-dev && npm run start-dev",
    "prod": "npm run build-prod && NODE_ENV=production ./bin/cnc",
    "lint": "npm run eslint && npm run stylint",
    "eslint": "eslint --ext .js --ext .jsx *.js src gulp test",
    "eslint:debug": "echo \"Checking code style...\"; DEBUG=eslint:cli-engine eslint --ext .js --ext .jsx *.js src gulp test",
    "stylint": "stylint src/web",
    "test": "tap test/*.js --no-timeout --node-arg=--require --node-arg=babel-register --node-arg=--require --node-arg=babel-polyfill",
    "coveralls": "tap test/*.js --coverage --coverage-report=text-lcov --nyc-arg=--require --nyc-arg=babel-register --nyc-arg=--require --nyc-arg=babel-polyfill | coveralls"
  },
  "build": {
    "appId": "com.snapmaker",
    "asar": false,
    "asarUnpack": [],
    "productName": "Snapmakerjs",
    "directories": {
      "buildResources": "electron-build",
      "output": "output",
      "app": "dist/cnc"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg"
      ],
      "icon": "electron-build/snap-icon.icns"
    },
    "dmg": {
      "background": "electron-build/background.png",
      "icon": "electron-build/snap-icon.icns",
      "iconSize": 80,
      "iconTextSize": 12,
      "contents": [
        {
          "x": 448,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 344,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "electron-build/snap-icon.ico"
    },
    "linux": {
      "category": "Utility",
      "maintainer": "parachvte <ryan@snapmaker.com>",
      "target": [
        "deb"
      ]
    }
  },
  "keywords": [
    "snapmaker",
    "snapmakerjs",
    "cnc",
    "gcode",
    "marlin"
  ],
